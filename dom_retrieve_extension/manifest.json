{
  "name": "DOM Retriever Extensions",
  "description" : "Base Level Extension",
  "version": "1.0",
  "browser_action": {
    "default_title": "Test Extension",
    "default_icon": "dom_extension.png"
  },
  "manifest_version": 2,

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [{
    "matches": ["*://*.stackoverflow.com/*"],
    "js": ["content.js"]
  }],
  "permissions": ["activeTab"]
}
